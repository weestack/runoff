pinid LED1 = 13P;
pinid LED2 = 8P;

messages {
	hello;
}

task ledhandler(pinid led, uint32 miliseconds){
	for(;;){
		turnOn(led);
		Delay(miliseconds);
		turnOff(led);
		Delay(miliseconds);
	}
}

function setup() -> void {
	pinMode(LED1, OUTPUT);
	pinMode(LED2, OUTPUT);
	uint32 a = 500;
	uint32 b = 1000;
	spawn ledhandler(LED1, a);
	spawn ledhandler(LED2, b);
}