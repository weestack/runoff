function sum(int[5] numbers) -> int {
	int res;
	int i;
	for(i = 0; i < 5; i++){
		res = res + numbers[i]; # Array indexing stats from 0
	}
	return res;
}

function test() -> void {
	int[5] numbers;
	numbers[0] = 1;
	numbers[1] = 2;
	numbers[2] = 3;
	numbers[3] = 4;
	numbers[4] = 5;
	int s = sum(numbers);
	# s is now = 15
}
